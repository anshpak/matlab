% Задание 2
% выполните вычисление и построение фигур Лиссажу для разных отношений и
% для разных значений амплтуд
a1 = [1.2, 10.4, -4.2, -8.5, 2.1];
a2 = [1.0, 4.4, -2.3, 1.8, -5.6];
w1 = [1.5, 2.2, 4.5, -3];
